{
  "name": "@auriclabs/tomlette",
  "version": "0.0.1",
  "description": "A flexible parser for TOML/YAML/Properties files with variable substitution and JSON5-like type inference",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "prettier": "@auriclabs/prettier-config",
  "keywords": ["toml", "yaml", "properties", "parser", "config", "variables"],
  "author": "",
  "type": "module",
  "license": "ISC",
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts --tsconfig tsconfig.build.json",
    "dev": "concurrently \"pnpm build --watch\" \"pnpm:y:watch\"",
    "y:watch": "chokidar dist --initial --silent -c \"yalc publish --push\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "ts-config-typecheck",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@auriclabs/logger": "workspace:*",
    "lodash-es": "4.17.21",
    "toml": "3.0.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.12",
    "@types/yaml": "^2.6.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/tomlette"
  }
}
