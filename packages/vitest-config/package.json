{
  "name": "@auriclabs/vitest-config",
  "version": "0.0.1",
  "description": "Vitest config for AuricLabs projects",
  "prettier": "@auriclabs/prettier-config",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/node.config.d.ts",
      "import": "./dist/node.config.js",
      "require": "./dist/node.config.cjs"
    },
    "./node": {
      "types": "./dist/node.config.d.ts",
      "import": "./dist/node.config.js",
      "require": "./dist/node.config.cjs"
    },
    "./sst": {
      "types": "./dist/sst.config.d.ts",
      "import": "./dist/sst.config.js",
      "require": "./dist/sst.config.cjs"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "tsdown src/* src/setup/* --dts",
    "dev": "concurrently \"pnpm build --watch\" \"pnpm:y:watch\"",
    "y:watch": "chokidar dist --initial --silent -c \"yalc publish --push\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vite": "7.1.7",
    "vitest": "^3.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/vitest-config"
  },
  "devDependencies": {
    "vite": "7.1.7",
    "vitest": "3.2.4"
  }
}
