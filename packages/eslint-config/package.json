{
  "name": "@auriclabs/eslint-config",
  "version": "0.0.6",
  "description": "Common ESLint configuration for AuricLabs packages",
  "prettier": "@auriclabs/prettier-config",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "types": "./types.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "exports": {
    "./lint-staged": "./configs/lint-staged.mjs",
    "./common-ts": {
      "types": "./types.d.ts",
      "default": "./configs/common-ts.mjs"
    },
    "./common-js": {
      "types": "./types.d.ts",
      "default": "./configs/common-js.mjs"
    },
    "./node-ts": {
      "types": "./types.d.ts",
      "default": "./configs/node-ts.mjs"
    },
    "./node-js": {
      "types": "./types.d.ts",
      "default": "./configs/node-js.mjs"
    },
    "./react": {
      "types": "./types.d.ts",
      "default": "./configs/react.mjs"
    },
    "./sst": {
      "types": "./types.d.ts",
      "default": "./configs/sst.mjs"
    },
    "./vitest": {
      "types": "./types.d.ts",
      "default": "./configs/vitest.mjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@eslint/js": ">=9.0.0",
    "@typescript-eslint/eslint-plugin": ">=8.0.0",
    "@typescript-eslint/parser": ">=8.0.0",
    "eslint": ">=9.0.0",
    "eslint-config-prettier": ">=10.0.0",
    "eslint-import-resolver-typescript": ">=4.0.0",
    "eslint-plugin-import": ">=2.0.0",
    "eslint-plugin-n": ">=17.0.0",
    "eslint-plugin-prettier": ">=5.0.0",
    "eslint-plugin-react": ">=7.0.0",
    "eslint-plugin-react-hooks": ">=4.0.0",
    "typescript-eslint": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/eslint-config"
  },
  "dependencies": {
    "eslint-plugin-vitest": "0.5.4"
  }
}
