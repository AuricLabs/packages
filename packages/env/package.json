{
  "name": "@auriclabs/env",
  "version": "0.0.3",
  "description": "Environment loading and validation",
  "prettier": "@auriclabs/prettier-config",
  "keywords": [],
  "author": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": {
    "dotenv": false
  },
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./configure": {
      "types": "./dist/configure.d.ts",
      "import": "./dist/configure.js",
      "require": "./dist/configure.cjs"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts src/configure.ts --format cjs,esm --dts",
    "dev": "concurrently \"pnpm build --watch\" \"pnpm:y:watch\"",
    "y:watch": "chokidar dist --initial --silent -c \"yalc publish --push\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "ts-config-typecheck",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/env"
  },
  "peerDependencies": {
    "zod": "4.1.5"
  },
  "dependencies": {
    "dotenv": "17.2.2"
  }
}
