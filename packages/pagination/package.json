{
  "name": "@auriclabs/pagination",
  "version": "0.0.8",
  "description": "Pagination for AuricLabs projects",
  "prettier": "@auriclabs/prettier-config",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [],
  "author": "",
  "type": "module",
  "license": "ISC",
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts",
    "dev": "concurrently \"pnpm build --watch\" \"pnpm:y:watch\"",
    "y:watch": "chokidar dist --initial --silent -c \"yalc publish --push\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "ts-config-typecheck",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@auriclabs/logger": "workspace:*"
  },
  "peerDependencies": {
    "@auriclabs/logger": "^0.0.7"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/pagination"
  }
}
