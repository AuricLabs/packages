{
  "name": "@auriclabs/logger",
  "version": "0.0.4",
  "description": "Logger for AuricLabs projects",
  "prettier": "@auriclabs/prettier-config",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [],
  "author": "",
  "type": "module",
  "license": "ISC",
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts",
    "dev": "concurrently \"pnpm build --watch\" \"pnpm:y:watch\"",
    "y:watch": "chokidar dist --initial --silent -c \"yalc publish --push\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "ts-config-typecheck",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "lodash-es": "4.17.21",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@auriclabs/env": "workspace:*",
    "@types/axios": "^0.14.4",
    "@types/lodash-es": "4.17.12"
  },
  "peerDependencies": {
    "@auriclabs/env": "^0.0.3",
    "axios": "^1.12.1",
    "pino-lambda": "^4.4.1",
    "sst": "^3.17.10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/api-core"
  }
}