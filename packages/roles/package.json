{
  "name": "@auriclabs/roles",
  "version": "0.0.7",
  "description": "User abilities and roles library",
  "prettier": "@auriclabs/prettier-config",
  "keywords": [],
  "author": "",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "license": "ISC",
  "type": "module",
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts",
    "dev": "concurrently \"pnpm build --watch\" \"pnpm:y:watch\"",
    "y:watch": "chokidar dist --initial --silent -c \"yalc publish --push\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "ts-config-typecheck",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "escape-string-regexp": "^4.0.0",
    "json5": "^2.2.3",
    "lodash-es": "4.17.21",
    "sift": "17.1.3",
    "zod": "^4.0.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/roles"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.12"
  }
}
