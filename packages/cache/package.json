{
  "name": "@auriclabs/cache",
  "version": "0.0.9",
  "description": "Cache for AuricLabs projects",
  "prettier": "@auriclabs/prettier-config",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts",
    "dev": "concurrently \"pnpm build --watch\" \"pnpm:y:watch\"",
    "y:watch": "chokidar dist --initial --silent -c \"yalc publish --push\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "ts-config-typecheck",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "keyv": "5.5.1",
    "promise-inflight": "1.0.1",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "@auriclabs/logger": "^0.0.6"
  },
  "devDependencies": {
    "@auriclabs/logger": "workspace:*",
    "@types/promise-inflight": "1.0.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auriclabs/packages.git",
    "directory": "packages/cache"
  }
}
